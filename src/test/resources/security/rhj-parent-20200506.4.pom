	<properties>
	
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		
		<maven.compiler.source>11</maven.compiler.source>
		<maven.compiler.target>11</maven.compiler.target>
		
		<!-- https://mvnrepository.com/artifact/org.bouncycastle/bcpg-jdk15on -->
		<bcpg-jdk15on.version>1.65</bcpg-jdk15on.version>
		
		<commons-compress.version>1.20</commons-compress.version>
		<commons-io.version>2.6</commons-io.version>
		<commons-lang3.version>3.10</commons-lang3.version>
		
		<!-- https://mvnrepository.com/artifact/org.apache.commons/commons-text -->
		<commons-text.version>1.8</commons-text.version>
		
		<!-- https://mvnrepository.com/artifact/com.fasterxml.jackson.core/jackson-annotations -->
		<jackson-annotations.version>2.11.0</jackson-annotations.version>
		
		<!-- https://mvnrepository.com/artifact/com.fasterxml.jackson.core/jackson-databind -->
		<jackson-databind.version>2.11.0</jackson-databind.version>
		
		<jackson-dataformat-yaml.version>2.11.0</jackson-dataformat-yaml.version>
		<java.version>1.8</java.version>
		<jimfs.version>1.1</jimfs.version>
